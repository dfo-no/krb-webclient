    "@fontsource/source-sans-pro": "^4.5.11",
    "@fontsource/source-serif-pro": "^4.5.9",
    "@hookform/devtools": "^4.3.0",
    "@hookform/resolvers": "^2.9.10",
    "@microsoft/applicationinsights-react-js": "^3.4.0",
    "@microsoft/applicationinsights-web": "^2.8.9",
