    "@mui/material": "^5.11.4",
    "@mui/styles": "^5.11.2",
    "@mui/x-date-pickers": "^5.0.13",
    "@reduxjs/toolkit": "^1.9.1",
    "async-sema": "^3.1.1",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "history": "^5.3.0",
    "i18next": "^21.10.0",
    "i18next-browser-languagedetector": "^6.1.8",
    "i18next-http-backend": "^1.4.5",
    "immer": "^9.0.18",
    "joi": "^17.7.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "openapi-typescript-fetch": "^1.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.41.5",
    "react-i18next": "^11.18.6",
    "react-nestable": "^2.0.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^5.3.4",
    "swr": "^2.0.1",
    "unstated-next": "^1.1.0",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.1.0",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.191",
    "@types/node": "^17.0.45",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.3.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "pre-commit": "^1.2.2",
    "prettier": "2.8.2",
    "react-scripts": "5.0.1",
    "sass-embedded": "^1.57.1",
    "timezone-mock": "^1.3.6",
    "typescript": "^4.9.4",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "overrides": {
    "fbjs@<1.0.0": {
      "isomorphic-fetch": {
        "node-fetch": "^2.6.7"
      }
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:ci": "cross-env CI=false react-scripts build",
    "test": "react-scripts test",
    "test:ci": "cross-env CI=true react-scripts test",
    "test:coverage": "cross-env CI=true react-scripts test --coverage",
    "eslint": "eslint 'src/**/*.{js,jsx,ts,tsx,json,css,md}'",
    "lint:ci": "tsc --noEmit && eslint ./ --ignore-path .gitignore",
    "lint:fix": "tsc --noEmit && eslint ./ --ignore-path .gitignore --fix",
    "prettier:ci": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prettier:write": "prettier  --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "git-info": "echo \"{\\\"sha\\\": \\\"$(git show -s --format='%h')\\\", \\\"msg\\\": \\\"$(git show -s --format='%s')\\\"}\"  > public/version.json",
    "analyse": "node analyse.js"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-typescript",
      "airbnb/hooks",
      "plugin:@typescript-eslint/recommended",
      "plugin:jest/recommended",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "react",
      "@typescript-eslint",
      "jest",
      "import",
      "jsx-a11y"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "rules": {
      "no-param-reassign": [
        "error",
        {
          "props": true,
          "ignorePropertyModificationsFor": [
            "state",
            "draft"
          ]
        }
      ],
      "spaced-comment": "warn",
      "import/order": [
        "warn",
        {
          "groups": [
            [
              "external",
              "internal"
            ]
          ],
          "newlines-between": "always"
        }
      ],
      "linebreak-style": "off",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ],
      "react/jsx-props-no-spreading": 0
    },
    "ignorePatterns": [
      "analyse.js",
      "public/OidcServiceWorker.js",
      "src/api/generated.ts"
    ],
    "overrides": [
      {
        "files": [
          "cypress.config.ts",
          "setupTests.ts",
          "test-utils.tsx"
        ],
        "rules": {
          "import/no-extraneous-dependencies": [
            "error",
            {
              "devDependencies": true
            }
          ]
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pre-commit": [
    "prettier:ci",
    "test:ci"
  ],
  "volta": {
    "node": "16.18.0",
    "npm": "8.19.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/e2e/common/*.{js,ts}"
  }
}
